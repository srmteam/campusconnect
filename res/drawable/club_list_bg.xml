<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#FFFDE7" />
            <stroke
                android:width="1dp"
                android:color="#FFF" />
            <corners
                android:radius="1dp" 
                />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#FFF"
                android:endColor="#FFF"
                android:angle="90" />
            <stroke
                android:width="1dp"
                android:color="#FFF" />
            <corners
                android:radius="1dp" />

        </shape>
    </item>
</selector>